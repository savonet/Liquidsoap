(executable
 (name liquidsoap)
 (public_name liquidsoap)
 (package liquidsoap)
 (preprocess
  (pps ppx_string))
 (link_flags -cclib %{env:LDFLAGS=})
 (libraries
  liquidsoap_core
  liquidsoap_optionals
  liquidsoap_builtins
  (select
   runner.ml
   from
   (winsvc -> runner.winsvc.ml)
   (-> runner.default.ml)))
 (modules liquidsoap runner main build_config))

(executable
 (name sys_config)
 (modules sys_config)
 (link_flags -cclib %{env:LDFLAGS=})
 (libraries dune-configurator))
