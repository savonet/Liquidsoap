#!/usr/bin/make -f

DESTDIR := debian/tmp

%:
	dh $@

override_dh_autoreconf:
	/bin/true

override_dh_auto_configure:
	/bin/true

override_dh_auto_build:
	/bin/true

override_dh_auto_test:
	/bin/true

override_dh_auto_install:
	ls -lh /tmp/debian
	mkdir -p $(DESTDIR)
	cp -rf /tmp/debian/* $(DESTDIR)
	dh_install

override_dh_auto_clean:
	/bin/true
